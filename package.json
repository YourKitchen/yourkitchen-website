  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "scripts": {
    "start": "craco start",
    "sitemap": "node ./index.js",
    "prebuild": "cd generate-sitemap && yarn && yarn start",
    "build_local": "BUILD_PATH='./render-server/build' craco build",
    "build": "BUILD_PATH='./render-server/build' PUBLIC_URL=https://yourkitchen.io craco build",
    "test": "craco test",
    "prepare": "husky install",
    "lint": "eslint --fix . && prettier --write . --ignore-unknown",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*": "prettier --write --ignore-unknown"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
