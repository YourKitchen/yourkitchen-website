    "@biomejs/biome": "2.3.14",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@types/luxon": "^3.7.1",
    "@types/node": "25.2.2",
    "@types/nodemailer": "^7.0.9",
    "@types/react": "19.2.13",
    "@types/uuid": "^11.0.0",
    "eventsource": "^4.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prisma": "^7.3.0",
    "typescript": "^5.9.3"
