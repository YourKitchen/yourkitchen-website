{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2022",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "paths": {
      "#assets/*": [
        "./src/assets/*"
      ],
      "#components/*": [
        "./src/components/*"
      ],
      "#pages/*": [
        "./pages/*"
      ],
      "#models/*": [
        "./src/models/*"
      ],
      "#contexts/*": [
        "./src/contexts/*"
      ],
      "#network/*": [
        "./src/network/*"
      ],
      "#utils/*": [
        "./src/utils/*"
      ],
      "#misc/*": [
        "./src/misc/*"
      ],
      "#prisma": [
        "./src/misc/prisma.ts"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "scripts/*.ts",
    "types",
    "build/types/**/*.ts",
    ".next/types/**/*.ts",
    "commitlint.config.ts",
    "build/dev/types/**/*.ts",
    ".intlayer/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}
